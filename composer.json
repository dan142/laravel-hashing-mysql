{
    "name": "dan142/laravel-hashing-mysql",
    "type": "library",
    "keywords": ["laravel", "hashing", "mysql", "password", "old_password", "hasher"],
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Crossley",
            "email": "me@danielcrossley.com"
        }
    ],
    "config": {
        "allow-plugins": false
    },
    "require": {
        "php" : "^7.3|^8.0",
        "illuminate/support": "^5.6|^6.0|^7.0|^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^8.0|^9.0|^10.0",
        "orchestra/testbench": "^3.6|^4.0|^5.0|^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Dan142\\LaravelHashingMySql\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dan142\\LaravelHashingMySql\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dan142\\LaravelHashingMySql\\LaravelHashingMySqlServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
